# =============================================================================
# DOCTAI HEALTH HUB - ENVIRONMENT CONFIGURATION EXAMPLE
# =============================================================================
# Copy this file to .env and replace placeholder values with your actual credentials
# NEVER commit the actual .env file to version control

# ==============================================================================
# FITNESS INTEGRATIONS
# ==============================================================================
# Google Fit OAuth 2.0 Credentials
VITE_GOOGLE_FIT_CLIENT_ID=your_google_fit_client_id_here
VITE_GOOGLE_FIT_CLIENT_SECRET=your_google_fit_client_secret_here

# Fitbit OAuth 2.0 Credentials
VITE_FITBIT_CLIENT_ID=your_fitbit_client_id_here
VITE_FITBIT_CLIENT_SECRET=your_fitbit_client_secret_here

# =============================================================================
# CLOUD HEALTHCARE APIs
# =============================================================================
# Primary cloud healthcare provider (google, azure, watson)
VITE_PRIMARY_CLOUD_PROVIDER=google

# Enable cloud healthcare features
VITE_USE_CLOUD_HEALTHCARE=true
VITE_CLOUD_HEALTHCARE_FALLBACK=true
VITE_ENABLE_CONSENSUS_ANALYSIS=false

# Google Cloud Healthcare
VITE_GOOGLE_HEALTHCARE_PROJECT_ID=your_google_healthcare_project_id
VITE_GOOGLE_HEALTHCARE_LOCATION=us-central1
VITE_GOOGLE_HEALTHCARE_DATASET_ID=your_healthcare_dataset_id
VITE_GOOGLE_HEALTHCARE_API_KEY=your_google_healthcare_api_key
VITE_GOOGLE_CLOUD_STORAGE_BUCKET=your_storage_bucket_name

# Google Maps API
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# Azure Health Bot (optional)
VITE_AZURE_HEALTH_BOT_ENDPOINT=https://your-bot.azurewebsites.net
VITE_AZURE_HEALTH_BOT_API_KEY=your_azure_health_bot_api_key

# IBM Watson Health (optional)
VITE_WATSON_HEALTH_API_KEY=your_watson_api_key
VITE_WATSON_HEALTH_ENDPOINT=https://your-watson-instance.com
VITE_WATSON_HEALTH_VERSION=2023-01-01

# =============================================================================
# AI & COMMUNICATION APIs
# =============================================================================
# OpenAI API
VITE_OPENAI_API_KEY=your_openai_api_key_here
VITE_OPENAI_MODEL=gpt-4

# Twilio API (SMS notifications)
VITE_TWILIO_ACCOUNT_SID=your_twilio_account_sid
VITE_TWILIO_AUTH_TOKEN=your_twilio_auth_token
VITE_TWILIO_PHONE_NUMBER=+1234567890

# SendGrid API (Email notifications)
VITE_SENDGRID_API_KEY=your_sendgrid_api_key
VITE_SENDGRID_FROM_EMAIL=noreply@yourdomain.com

# =============================================================================
# DATABASE & AUTHENTICATION
# =============================================================================
# Supabase Configuration
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# =============================================================================
# ML & PREDICTION SERVICES
# =============================================================================
VITE_USE_NEW_PREDICTION_API=true
VITE_DEBUG_PREDICTIONS=false
VITE_ML_API_ENDPOINT=http://localhost:8000/api/predict
VITE_PROGRESSION_API_ENDPOINT=http://localhost:8000/api/progression
VITE_VITAL_SIGNS_API_ENDPOINT=http://localhost:8000/api/vital-signs

# Google Cloud Service Account (for ML services)
GOOGLE_APPLICATION_CREDENTIALS=./path/to/your-service-key.json

# =============================================================================
# PERFORMANCE & DEBUG SETTINGS
# =============================================================================
VITE_CLOUD_HEALTHCARE_TIMEOUT=30000
VITE_CLOUD_HEALTHCARE_MAX_RETRIES=3
VITE_CLOUD_HEALTHCARE_DEBUG=false
VITE_CLOUD_HEALTHCARE_LOG_REQUESTS=false
